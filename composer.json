{
    "name": "bolt/slack-invites",
    "description": "Slack invitation generator site for Bolt.cm Slack channels",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "bolt/collection": "^1.0",
        "bolt/common": "^1.0",
        "hostnet/form-handler-bundle": "^1.5",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug-pack": "^1.0",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/security-csrf": "^4.0",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/web-profiler-bundle": "^4.0",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "bolt/codingstyle": "^2.0@dev",
        "escapestudios/symfony2-coding-standard": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Bolt\\Site\\SlackInvites\\": "src"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "02C1ETCDQKE0S7M1CF2JP4C0ZA",
            "allow-contrib": false
        }
    }
}
